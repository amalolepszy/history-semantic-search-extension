{
  "manifest_version": 3,
  "name": "History Semantic Search",
  "description": "Browse history semantically in the side panel",
  "version": "1.0",
  "permissions": ["history", "sidePanel", "storage"],
  "side_panel": {
    "default_path": "index.html"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "action": {
    "default_title": "Open History"
  },
  "icons": {
    "16": "logo192.png",
    "32": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "host_permissions": [
    "https://api.openai.com/*",
    "https://generativelanguage.googleapis.com/*"
  ]
}