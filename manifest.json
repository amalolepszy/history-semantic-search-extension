{
  "manifest_version": 3,
  "name": "History Side Panel",
  "version": "1.1",
  "permissions": [
    "history",
    "sidePanel"
  ],
  "side_panel": {
    "default_path": "panel.html"
  },
  "action": {
    "default_title": "Click to open history"
  },
  "background": {
    "service_worker": "service-worker.js"
  }
}